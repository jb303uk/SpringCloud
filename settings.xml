<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.2.0 http://maven.apache.org/xsd/settings-1.2.0.xsd" xmlns="http://maven.apache.org/SETTINGS/1.2.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <servers>
    <server>
      <username>${env.SERVER_USERNAME}</username>
      <password>${env.SERVER_PASSWORD}</password>
      <id>${env.SERVER_ID}</id>
    </server>
  </servers>
<profiles>
    <profile>
	<id>jb303</id>
      <repositories>
        <repository>
          <id>central</id>
          <url>https://repo1.maven.org/maven2</url>
        </repository>
        <repository>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
          <id>jb303</id>
          <name>default-maven-virtual</name>
          <url>https://maven.pkg.github.com/jb303uk/SpringCloud</url>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
          <id>jb303</id>
          <name>default-maven-virtual</name>
          <url>https://maven.pkg.github.com/jb303uk/SpringCloud</url>
        </pluginRepository>
      </pluginRepositories>      
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>jb303</activeProfile>
  </activeProfiles>
</settings>
